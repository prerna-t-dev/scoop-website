        "item_pHNFh7": {
          "type": "item",
          "settings": {
            "image": "shopify://shop_images/fit-philosophy--2_1.png",
            "show_controls": false,
            "column_size": "3",
            "title": "We don’t just calculate your size",
            "heading_tag": "h4",
            "content": "<p>When you share your body measurements and details with us through our bra size quiz, we use that information to understand your unique shape and recommend styles that are most likely to support, lift, and feel good on you.<br/></p>",
            "link_url": "",
            "link_text": "",
