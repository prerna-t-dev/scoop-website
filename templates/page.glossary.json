    "bra_parts_and_common_issues_PjBNgJ": {
      "type": "bra-parts-and-common-issues",
      "blocks": {
        "bra_part_4HQGEh": {
          "type": "bra_part",
          "settings": {
            "part_name": "Cups",
            "description": "The cups are where your breast tissue sits. They can be lined or unlined, seamed or seamless, and play a key role in shaping and supporting your breasts.",
            "common_issues": "Breast spillage/ bulging, Gaping cups, Chaffing or cutting in under the armpit"
          }
        },
        "bra_part_GwQjkq": {
          "type": "bra_part",
          "settings": {
            "part_name": "Band",
            "description": "The band is the part of the bra that wraps around your torso — and it's responsible for providing about 80% of the support. A well-fitting band is the foundation of a supportive bra.",
            "common_issues": "Band rides up, Band digs in, Band easily pulled away from the body, Back pain"
          }
        },
        "bra_part_mjBwJT": {
          "type": "bra_part",
          "settings": {
            "part_name": "Underwire",
            "description": "Usually made of metal or plastic, underwires run along the bottom and sides of the cups. They help define the bra's shape and create an uplifted silhouette.",
            "common_issues": "Poking or Digging in of wires"
          }
        },
        "bra_part_xwEgXV": {
          "type": "bra_part",
          "settings": {
            "part_name": "Gore",
            "description": "The gore is the small panel at the centre front of the bra that connects the two cups. In an ideal fit, the gore should sit flat against your sternum.",
            "common_issues": "Floating gore, Gore digging into the sternum"
          }
        },
        "bra_part_EpWaXh": {
          "type": "bra_part",
          "settings": {
            "part_name": "Wings",
            "description": "These are the side and back portions of the bra that stretch from the outer edge of the cups to the hooks or closures. They help anchor the bra and contribute to a smooth fit under clothing.",
            "common_issues": "Being able to wear a new bra on the tightest hook"
          }
        },
        "bra_part_gCbcr8": {
          "type": "bra_part",
          "settings": {
            "part_name": "Straps",
            "description": "Straps help keep your bra secure and stable, but they're only meant to carry around 20% of the weight of your breasts. They're almost always adjustable and should be set to a two finger tension for optimal support.",
            "common_issues": "Loose straps, Straps sliding off, Shoulder pain/ tension"
          }
        }
      },
